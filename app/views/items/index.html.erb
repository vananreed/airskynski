<div class="container">
  <h2>Search Results</h2>
</div>

<div class="container">
  <div class="row">
    <% @items.each do |item| %>
      <% if item.published %>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
        <%= item.name %>
        <%= item.description %>
        <p>Send <%= item.user.first_name %> a message </p>
        <%= link_to "See Details", item_path(item) %>
        </div>
      <% end %>
    <% end %>
  </div>
</div>

<div class="container">
  <%= link_to "List eQuipment", new_item_path %>
</div>

