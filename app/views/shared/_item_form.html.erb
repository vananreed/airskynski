<%= simple_form_for @item do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :category, collection: ["Snow", "Water", "Mountain", "Sky", "Wind", "Outdoor"] %>
  <%= f.input :photo %>
  <%= f.input :address %>
  <%= f.input :published %>
  <%= f.submit 'Publish'%>
<% end %>
<%= link_to "Cancel", :back %>
